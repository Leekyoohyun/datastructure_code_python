    "def reverse_list(lst):\n",
    "    n = len(lst)\n",
    "    for i in range(n // 2):\n",
    "        lst[i], lst[n - 1 - i] = lst[n - 1 - i], lst[i]\n",
    "    return lst\n",
    "\n",
    "example = [1, 2, 3, 4, 5]\n",
    "print(reverse_list(example))  # 출력: [5, 4, 3, 2, 1]\n"
   ]
  }
